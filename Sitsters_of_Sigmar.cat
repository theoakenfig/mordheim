      <selectionEntries>
        <selectionEntry id="9143d1c5-10e6-1cf1-71ce-e2d47f5d20e2" name="Sign of Sigmar" page="0" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="parent" value="1" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="53ea-fe0a-2227-1986" type="max"/>
          </constraints>
          <rules>
            <rule id="ec0201c9-9adc-5e88-1532-5e06d7cd4dc5" name="Sign of Sigmar" page="0" hidden="false">
              <description>Possessed or Undead opponents lose their first attack against the Priestess in the first round of hand-to-hand combat (down to a minimum of 1).</description>
            </rule>
          </rules>
          <costs>
            <cost name="pts" typeId="points" value="0"/>
