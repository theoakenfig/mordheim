        <selectionEntry id="81e79a9d-28af-bac2-be55-b8ed2f950f34" name="Absolute Faith" page="0" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="parent" value="1" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="5053-3fb6-7d9c-e124" type="max"/>
          </constraints>
          <rules>
            <rule id="0fbdda54-9bea-b5cb-e0ca-dfaaefc0554e" name="Absolute Faith" page="0" hidden="false">
              <description>She may re-roll any Fear tests and does not have to test if she is fighting alone against several opponents.</description>
